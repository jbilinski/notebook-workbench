
{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "jekyll",
    "headers": [
        {
            "source": "/(.*)", #https://drive.usercontent.google.com
            "headers": [
                {
                    "key": "sec-fetch-mode",
                    "value": "no-cors"
                },
                {
                    "key": "sec-fetch-site",
                    "value": "cross-site"
                }
            ]
        }
    ]
}
