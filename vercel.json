
{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "jekyll",
    "headers": [
        {
            "source": "/(.*)", 
            "headers": [
                {
                  "key": "Access-Control-Allow-Origin",
                  "value": "https://drive.usercontent.google.com"
                },
                {
                  "key": "Access-Control-Allow-Methods",
                  "value": "GET,OPTIONS"
                },
                {
                  "key": "Access-Control-Allow-Headers",
                  "value": "Content-Type"
                },
                {
                    "key": "sec-fetch-mode",
                    "value": "no-cors"
                },
                {
                    "key": "sec-fetch-site",
                    "value": "cross-site"
                }
            ]
        }
    ]
}
